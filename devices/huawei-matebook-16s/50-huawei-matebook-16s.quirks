# This quirk enables palm detection and some other features probably
# copy this file to /usr/share/libinput/

[Huawei MateBook 16s 2023 Touchpad]
MatchName=GXTP7863:00 27C6:01E8
MatchUdevType=touchpad
# see /sys/class/dmi/id/modalias
MatchDMIModalias=dmi:*svnHUAWEI:*pvrM1010*
AttrEventCode=-BTN_RIGHT

